<?xml version="1.0" encoding="UTF-8"?>
<project name="module_polychatbukkitclient" default="compile.module.polychatbukkitclient">
  <dirname property="module.polychatbukkitclient.basedir" file="${ant.file.module_polychatbukkitclient}"/>
  
  <property name="module.jdk.home.polychatbukkitclient" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.polychatbukkitclient" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.polychatbukkitclient" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.polychatbukkitclient" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="polychatbukkitclient.output.dir" value="${module.polychatbukkitclient.basedir}/../../Spigot/plugins/production/PolyChatBukkitClient"/>
  <property name="polychatbukkitclient.testoutput.dir" value="${module.polychatbukkitclient.basedir}/../../Spigot/plugins/test/PolyChatBukkitClient"/>
  
  <path id="polychatbukkitclient.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="polychatbukkitclient.module.production.classpath">
    <path refid="${module.jdk.classpath.polychatbukkitclient}"/>
    <pathelement location="${basedir}/../../../../Downloads/spigot-api-1.14.4-R0.1-20191210.224154-92.jar"/>
    <path refid="library.networklibrary.classpath"/>
  </path>
  
  <path id="polychatbukkitclient.runtime.production.module.classpath">
    <pathelement location="${polychatbukkitclient.output.dir}"/>
    <pathelement location="${basedir}/../../../../Downloads/spigot-api-1.14.4-R0.1-20191210.224154-92.jar"/>
    <path refid="library.networklibrary.classpath"/>
  </path>
  
  <path id="polychatbukkitclient.module.classpath">
    <path refid="${module.jdk.classpath.polychatbukkitclient}"/>
    <pathelement location="${polychatbukkitclient.output.dir}"/>
    <pathelement location="${basedir}/../../../../Downloads/spigot-api-1.14.4-R0.1-20191210.224154-92.jar"/>
    <path refid="library.networklibrary.classpath"/>
  </path>
  
  <path id="polychatbukkitclient.runtime.module.classpath">
    <pathelement location="${polychatbukkitclient.testoutput.dir}"/>
    <pathelement location="${polychatbukkitclient.output.dir}"/>
    <pathelement location="${basedir}/../../../../Downloads/spigot-api-1.14.4-R0.1-20191210.224154-92.jar"/>
    <path refid="library.networklibrary.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.polychatbukkitclient">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.polychatbukkitclient">
    <patternset refid="excluded.from.module.polychatbukkitclient"/>
  </patternset>
  
  <path id="polychatbukkitclient.module.sourcepath">
    <dirset dir="${module.polychatbukkitclient.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.polychatbukkitclient" depends="compile.module.polychatbukkitclient.production,compile.module.polychatbukkitclient.tests" description="Compile module PolyChatBukkitClient"/>
  
  <target name="compile.module.polychatbukkitclient.production" depends="register.custom.compilers" description="Compile module PolyChatBukkitClient; production classes">
    <mkdir dir="${polychatbukkitclient.output.dir}"/>
    <javac2 destdir="${polychatbukkitclient.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.polychatbukkitclient}/javac">
      <compilerarg line="${compiler.args.polychatbukkitclient}"/>
      <bootclasspath refid="polychatbukkitclient.module.bootclasspath"/>
      <classpath refid="polychatbukkitclient.module.production.classpath"/>
      <src refid="polychatbukkitclient.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.polychatbukkitclient"/>
    </javac2>
    
    <copy todir="${polychatbukkitclient.output.dir}">
      <fileset dir="${module.polychatbukkitclient.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.polychatbukkitclient.tests" depends="register.custom.compilers,compile.module.polychatbukkitclient.production" description="compile module PolyChatBukkitClient; test classes" unless="skip.tests"/>
  
  <target name="clean.module.polychatbukkitclient" description="cleanup module">
    <delete dir="${polychatbukkitclient.output.dir}"/>
    <delete dir="${polychatbukkitclient.testoutput.dir}"/>
  </target>
</project>